{
  "name": "cf-cli-ga",
  "version": "0.0.0",
  "private": true,
  "description": "CF CLI github action",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "node ./util/ncc-build.js",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "build-n-package": "npm run build && npm run package",
    "test-act": "act workflow_dispatch -j test -b -P self-hosted=catthehacker/ubuntu:act-latest -v",
    "yaml-action-builder": "ts-node ./src/actions/builder.ts"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vercel/ncc": "^0.31.1",
    "act": "^0.0.6",
    "bytes": "^3.1.2",
    "eslint": "^8.0.30",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.1.7",
    "glob": "^8.0.3",
    "jest": "^27.2.5",
    "joi": "^17.7.0",
    "js-yaml": "^4.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
